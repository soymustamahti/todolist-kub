# Kubernetes Cluster Configuration
# Copy this file to .env and update with your actual values

# VM IP Addresses
MASTER_IP=192.168.1.10
WORKER1_IP=192.168.1.11
WORKER2_IP=192.168.1.12

# VM Authentication
ANSIBLE_USER=ubuntu
VM_PASSWORD=your_vm_password

# Individual passwords (optional - if different from VM_PASSWORD)
# MASTER_PASSWORD=master_specific_password
# WORKER1_PASSWORD=worker1_specific_password  
# WORKER2_PASSWORD=worker2_specific_password

# Docker Registry (for CI/CD)
DOCKER_USERNAME=your_docker_username
DOCKER_PASSWORD=your_docker_password

# Kubernetes Configuration
KUBERNETES_VERSION=1.28.2
POD_NETWORK_CIDR=10.244.0.0/16
SERVICE_CIDR=10.96.0.0/12
